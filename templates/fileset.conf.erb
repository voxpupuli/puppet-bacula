FileSet {
  Name = <%= @name %>
  Include {
    Options {
<% @options.each do |option,optval| -%>
<%   [optval].flatten.each do |value| -%>
<%=    "      #{option} = #{value}" %>
<%   end -%>
<% end -%>
    }
<% @files.each do |f| -%>
<%=    "    File = #{f}" %>
<% end -%>
  }
  Exclude {
<% @excludes.reject { |x| x.empty? }.each do |exclude| -%>
<%=    "    File = #{exclude}" %>
<% end -%>
  }
}

